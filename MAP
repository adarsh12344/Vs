#include <bits/stdc++.h>
#include<string>

using namespace std;

int main()
{
    
    int arr[5]={1,1,2,3,4};   
    map<int ,int>mpp;
    
    for(int i=0;i<5;i++)
    {
        mpp[arr[i]]++;   //hash[10000001]={0}
                        //for(i=0;i<n;i++){hash[arr[i]]++}    
    }
    
    
    int q;
    cin>>q;
    while(q--){
        int c;cin>>c;
         cout<<mpp[c]<<endl;        //hash[c]
    }
    
    

    return 0;
}
